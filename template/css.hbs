@font-face {
    font-family: "{{ fontName }}";
    src: {{{ src }}};
}

{{ baseTag }}[class*="{{classPrefix}}"] {
    line-height: 1;
}

{{ baseTag }}[class*="{{classPrefix}}"]:before {
    font-family: {{fontName}} !important;
    font-style: normal;
    font-weight: normal !important;
    vertical-align: baseline;
}

{{# each codepoints }}
.{{ ../classPrefix }}{{ @key }}:before {
    content: "\\{{ this }}";
}
{{/ each }}
